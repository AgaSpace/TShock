<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <LangVersion>7.3</LangVersion>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <Company>Pryaxis &amp; TShock Contributors</Company>
    <Copyright>Copyright © Pryaxis &amp; TShock Contributors 2011-2019</Copyright>
    <AssemblyVersion>4.3.26</AssemblyVersion>
    <FileVersion>4.3.26</FileVersion>
    <TargetFramework>netstandard2.0</TargetFramework>
    <LocalOTAPI Condition=" $(Configuration.Contains(LocalOTAPI)) ">true</LocalOTAPI>
    <LocalOTAPI Condition=" !$(Configuration.Contains(LocalOTAPI)) ">false</LocalOTAPI>
    <BinType Condition=" $(Configuration.StartsWith(Debug)) ">Debug</BinType>
    <BinType Condition=" $(Configuration.StartsWith(Release)) ">Release</BinType>
    <Configurations>Debug;Release;ReleaseLocalOTAPI;DebugLocalOTAPI</Configurations>
    <Platforms>AnyCPU</Platforms>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="..\TerrariaServerAPI\TerrariaServerAPI\TerrariaServerAPI.csproj" />
  </ItemGroup>
  <ItemGroup>
    <Reference Include="HttpServer, Version=2.0.0.0, Culture=neutral, PublicKeyToken=5684c2c737cd4271">
      <HintPath>..\prebuilts\HttpServer.dll</HintPath>
    </Reference>
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="BCrypt.Net-Core" Version="1.6.0" />
    <PackageReference Include="MySql.Data" Version="8.0.20" />
    <PackageReference Include="Newtonsoft.Json" Version="10.0.1" />
    <PackageReference Include="NLog" Version="4.7.0" />
    <PackageReference Include="System.Data.SQLite.Core" Version="1.0.112" />
    <PackageReference Include="SQLite" Version="3.13.0" />
    <PackageReference Include="System.Diagnostics.TraceSource" Version="4.3.0" />
    <PackageReference Include="System.Security.Cryptography.Algorithms" Version="4.3.1" />
    <PackageReference Include="System.Net.NameResolution" Version="4.3.0" />
    <PackageReference Include="System.Net.Primitives" Version="4.3.0" />
    <PackageReference Include="System.Threading.ThreadPool" Version="4.3.0" />
  </ItemGroup>
  <ItemGroup Condition=" $(LocalOTAPI) == true ">
    <Reference Include="OTAPI">
      <HintPath>$(ProjectDir)\..\TerrariaServerAPI\TShock.Modifications.Bootstrapper\bin\$(BinType)\Output\OTAPI.dll</HintPath>
    </Reference>
  </ItemGroup>
</Project>
